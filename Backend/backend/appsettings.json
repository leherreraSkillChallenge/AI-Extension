{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AIConfiguration": {
    "UseLocalAI": true,
    "UseLocalVision": false,
    "ProcessName": "Procesamiento local con Ollama"
  },
  "HttpClientTimeouts": {
    "OllamaTimeout": 30,
    "ChromaTimeout": 10,
    "OpenRouterTimeout": 30
  },
  "ConnectionStrings": {
    "ChromaServiceUrl": "http://chroma-service:9000",
    "OllamaUrl": "http://ollama-server:11434"
  }
}
